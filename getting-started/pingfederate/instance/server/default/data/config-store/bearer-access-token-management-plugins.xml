<?xml version="1.0" encoding="UTF-8"?>
<con:config xmlns:con="http://www.sourceid.org/2004/05/config">
    <con:map name="bearer-access-token-management-plugins">
        <con:item name="DelAuth">com.pingidentity.pf.access.token.management.plugins.JwtBearerAccessTokenManagementPlugin</con:item>
        <con:item name="default">org.sourceid.oauth20.token.plugin.impl.ReferenceBearerAccessTokenManagementPlugin</con:item>
        <con:item name="JWT">com.pingidentity.pf.access.token.management.plugins.JwtBearerAccessTokenManagementPlugin</con:item>
        <con:item name="OAuth">com.pingidentity.pf.access.token.management.plugins.JwtBearerAccessTokenManagementPlugin</con:item>
    </con:map>
    <con:item name="bearer-access-token-management-plugins-default">default</con:item>
    <con:map name="bearer-access-token-management-plugins-parents"/>
    <con:map name="bearer-access-token-management-plugin-sequence">
        <con:item name="DelAuth">1</con:item>
        <con:item name="default">4</con:item>
        <con:item name="JWT">2</con:item>
        <con:item name="OAuth">3</con:item>
    </con:map>
</con:config>